<?xml version="1.0" encoding="utf-8"?>
<!---
+  Skin for the default close button
-->
<s:Skin
   xmlns:fx="http://ns.adobe.com/mxml/2009" 
   xmlns:mx="library://ns.adobe.com/flex/mx" 
   xmlns:s="library://ns.adobe.com/flex/spark" 
   >
   
   <fx:Metadata>
      [HostComponent("spark.components.Button")] 
   </fx:Metadata>
   
   <fx:Script>
      <![CDATA[
         
      ]]>
   </fx:Script>
   
   <s:states>
      <s:State name="up" />
      <s:State name="over" />
      <s:State name="down" />
      <s:State name="disabled" />
   </s:states>
   
   <s:Image
      width="100%" height="100%"
      source.up="{ParsecEmbeds.CLOSE_BUTTON}"
      source.over="{ParsecEmbeds.CLOSE_BUTTON}"
      source.down="{ParsecEmbeds.CLOSE_BUTTON}"
      source.disabled="{ParsecEmbeds.CLOSE_BUTTON}"
      />
   
   <s:Rect top="0" bottom="0" left="0" right="0">
      <s:fill>
         <s:SolidColor alpha="0" color="0x00FFFF" />
      </s:fill>
   </s:Rect>
   
</s:Skin>
